
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/angular.js"></script>
<h2>Index</h2>
<div class="panel panel-default">
    <div class="panel-heading">AI编辑器</div>
    <div class="panel-body" style="text-align:center;padding:0px" id="gamepanel">
        <canvas id="Game" width="960" height="400" style="background-color:white"></canvas>
    </div>

</div>

<div class="panel panel-default" ng-app="codeEdit" ng-controller="codeCtrl">
    <div class="panel-heading">AI编辑器 <button type="button" class="btn btn-default" data-toggle="modal" data-target="#Market" ng-click="Codes()">看看别人家的代码</button></div>
    <div class="panel-body">
        <textarea class="form-control col-md-10" placeholder="输入代码" rows="5" id="code" ng-keyup="Reminder()" ng-model="CODE"></textarea>
        <select multiple class="form-control col-md-2">
            <option ng-repeat="item in words ">{{item}}</option>
        </select>
    </div>
    <div class="panel-footer">
        <div class="btn btn-default disabled" id="btn_code" onclick="Run()">运行</div>
        <div class="btn btn-primary " data-toggle="modal" data-target="#Upload">上传</div>
    </div>
    <div class="modal fade" id="Market" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">别人家的代码</h4>
                </div>
                @*<div class="modal-body">

                    </div>*@
                <table class="table">
                    <thead>
                        <tr>
                            <td>#</td>
                            <td>描述</td>
                            <td>作者</td>
                            @*<td>时间</td>*@
                            <td>操作</td>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="codeitem in codelist">
                            <td ng-bind="codeitem.Code_Id"></td>
                            <td ng-bind="codeitem.Code_Name"></td>
                            <td ng-bind="codeitem.Code_Author"></td>
                            @*<td ng-bind="codeitem.Code_CreateTime"></td>*@
                            <td><a ng-click="Use(codeitem)">使用</a></td>
                        </tr>
                    </tbody>
                </table>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>

                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="Upload" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">上传</h4>
                </div>
                <div class="modal-body">
                    <form>
                        <div class="form-group">
                            <label for="recipient-name" class="control-label">描述</label>
                            <input type="text" class="form-control" id="connectstring" placeholder="添加对代码的描述" ng-model="name">
                        </div>
                    </form>
                </div>

                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
                    <button type="button" class="btn btn-primary" ng-click="Save()">确认</button>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="runsection"></div>
<script>

    document.onkeydown = function () {
        if (event.keyCode == 9) {  //如果是其它键，换上相应在ascii 码即可。
            return false; //非常重要
        }
    }
    function insert_flg(str, flg, sn) {
        var newstr = "";
        for (var i = 0; i < str.length; i += sn) {
            var tmp = str.substring(i, i + sn);
            newstr += tmp + flg;
        }
        return newstr;
    }
    function compare(x, y) {
        var z = 0;
        var s = x.length + y.length;;

        //x.sort();
        //y.sort();
        //var a = x.shift();
        //var b = y.shift();

        //while (a !== undefined && b !== undefined) {
        //    if (a === b) {
        //        z++;
        //        a = x.shift();
        //        b = y.shift();
        //    } else if (a < b) {
        //        a = x.shift();
        //    } else if (a > b) {
        //        b = y.shift();
        //    }
        //}
        return z / s * 200;
    }
    function unique(arr) {
        var result = [], hash = {};
        for (var i = 0, elem; (elem = arr[i]) != null; i++) {
            if (!hash[elem]) {
                result.push(elem);
                hash[elem] = true;
            }
        }
        return result;
    }
    var myApp = angular.module('codeEdit', []);
    var KeyWordList = ["function", "var", "break", "do", "instanceof","typeof","case","else","new","var","catch","finally","return","void","continue","for","switch","while","this","with","default","if","throw","delete","in","try"];
    var Begin = [' ','.','\r','\n'];
    myApp.controller('codeCtrl', function ($scope, $http) {
        $scope.Codes = function () {
            $http.post("/game/GetCode",{uid:'@Guid.Parse("9349450C-BDD5-4239-A052-B08434D157AA").ToString()'}).success(function (Content) {
                $scope.codelist = Content;
                //console.log( Content);
            });
        }
        $scope.Save = function () {
            $http.post("/game/PostCode", { uid:'@Guid.Parse("9349450C-BDD5-4239-A052-B08434D157AA").ToString()',code: $scope.CODE, name: $scope.name }).success(function (data) {
                $('#Upload').modal('hide')
            });
        }
        $scope.Use = function (code) {
            $scope.CODE = code.Code_Content;
            $('#Market').modal('hide')
        }
        KeyWordList = unique(KeyWordList);






        $scope.Reminder = function () {


            //console.log(window.event.keyCode);
            var oTxt1 = document.getElementById("code");
            var cursurPosition = -1;
            if (oTxt1.selectionStart!==null) {//非IE浏览器
                cursurPosition = oTxt1.selectionStart;
            } else {//IE
                var range = document.selection.createRange();
                range.moveStart("character", -oTxt1.value.length);
                cursurPosition = range.text.length;
            }
            var startPosition = 0;
            var code= $scope.CODE;
            var selection = code.substring(0, cursurPosition + 1);
            //console.log(selection);

            for (var i = cursurPosition; i >= 0; i--) {
                if (Begin.indexOf(selection[i]) !== -1) {
                    word = selection.substring(i + 1, cursurPosition + 1);
                    //console.log(word);
                    break;
                }
            }
            var word = selection.substring(i + 1, cursurPosition + 1);

            if (window.event.keyCode ==9 && $scope.words != undefined) {
                //$scope.CODE += "" + $scope.words[0] + " ";
                var code = $scope.CODE.substring(0, i) +  $scope.words[0]  + $scope.CODE.substring(cursurPosition, $scope.CODE.length);
                $scope.CODE = code;
                    //insert_flg(code, " " + $scope.words[0] + " ", cursurPosition);

                return;
            }
            $scope.words = [];
            for(var i in KeyWordList){
                if (KeyWordList[i].indexOf(word) !== -1) {
                    $scope.words.push(KeyWordList[i]);

                }
            }
            $scope.words.sort(function (x,y) {
                return compare(x, word) - compare(y,word);
            });

            //console.log($scope.words);
            //console.log(cursurPosition);
        }

    });
</script>
