<script>

    var speed = 5;
    function OutOfBound(x, y) {
        if (x >= game.width || x <= 0) {
            return true;
        }
        if (y >= game.height|| y <= 0) {
            return true;
        }
        return false;
    }
    var Base = function () {
        this.container;
        this.shape;
        this.character;
        this.moved = false;
        this.Move = function (angle) {
            //console.log("move");
            //createjs.Tween.get(this.container).to({ rotation: angle }, 40, createjs.Ease.linear);
            if (this.moved) {
                this.character.play();
                var angle = angle * 2 * Math.PI / 360;
                var ss = Math.sin(angle);
                var next_x = this.container.x + 5 * Math.sin(angle);
                var next_y = this.container.y + 5 * Math.cos(angle);
                if (OutOfBound(next_x, next_y)) {
                    this.moved = false;
                } else {

                    createjs.Tween.get(this.container).to({ x: next_x, y: next_y }, 40, createjs.Ease.linear);
                    //this.container.y = next_y;
                }
            } else {
                this.character.stop(0);
            }
        }
        this.MoveTo = function (x, y,handleover) {
            if (OutOfBound(x, y)) {
                this.moved = false;
            } else {
                var s = (this.container.x - x) * (this.container.x - x) + (this.container.y - y) * (this.container.y - y);
                var distance = Math.sqrt(s);
                createjs.Tween.get(this.container).to({ x: x, y: y }, distance / speed * 100, createjs.Ease.backOut).call(handleover);
                //this.container.y = next_y;
            }
        }
        this.Speak = function (str) {
            var title = new createjs.Text(str, "8px Arial", "#777777");
            title.x = 30;
            title.y = 0;

            createjs.Tween.get(title).to({ alpha: 0 }, 1000, createjs.Ease.linear);
            this.container.addChild(title);
        }
    }
